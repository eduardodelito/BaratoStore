<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.enaz.baratostore.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/navigation_add"
        android:name="com.enaz.baratostore.add.AddFragment"
        android:label="@string/title_add"
        tools:layout="@layout/add_fragment" />

    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.enaz.baratostore.feed.FeedFragment"
        android:label="@string/title_feed"
        tools:layout="@layout/feed_fragment" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.enaz.baratostore.cart.CartFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/cart_fragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.enaz.baratostore.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/profile_fragment" />
</navigation>
